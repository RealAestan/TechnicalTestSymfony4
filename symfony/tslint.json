{
  "extends": "tslint:recommended",
  "rules": {
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ],
    "object-literal-sort-keys": false,
    "interface-name": [
      false
    ],
    "no-console": [
      false
    ],
    "forin": false,
    "prefer-for-of": false
  }
}
