{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": true,
    "sourceMap": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "types": [
      "jquery",
      "bootstrap",
      "typeahead"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules",
    "vendor"
  ]
}
